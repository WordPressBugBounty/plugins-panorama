{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "panorama/image-3d",
  "version": "1.0.0",
  "title": "Image 3D",
  "category": "widgets",
  "description": "Interactive 3D and 360Â° Viewer for Panoramic Images.",
  "keywords": [
    "panorama image viewer",
    "3D image viewer",
    "panorama image viewer"
  ],
  "textdomain": "panorama",
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "options": {
      "type": "object",
      "default": {
        "autoRotate": true,
        "autoRotateSpeed": 1,
        "autoRotateInactivityDelay": 3000,
        "hideDefaultCtrl": false,
        "initialView": false,
        "initialPosition": {
          "x": -61.42,
          "y": -8.95,
          "z": 120
        },
        "isDeviceMotion": false
      }
    },
    "layout": {
      "type": "object",
      "default": {
        "alignSl": {
          "desktop": "center",
          "tablet": "center",
          "mobile": "center"
        },
        "width": {
          "desktop": "100%",
          "tablet": "100%",
          "mobile": "100%"
        },
        "height": {
          "desktop": "320px",
          "tablet": "300px",
          "mobile": "270px"
        },
        "buttonColors": {
          "color": "#fff",
          "bg": "#000000B3"
        },
        "buttonHoverColors": {
          "color": "#fff",
          "bg": "#000000E6"
        }
      }
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {}
  },
  "editorScript": [
    "file:./index.js",
    "bppiv-panolens",
    "bppiv-three",
    "wp-api"
  ],
  "editorStyle": "file:./index.css",
  "style": "file:./view.css",
  "render": "file:./render.php",
  "viewScript": [
    "file:./view.js",
    "bppiv-panolens",
    "bppiv-three"
  ]
}